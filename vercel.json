{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.sinztools.app/$1",
      "statusCode": 301,
      "condition": {
        "host": "sinztools.app"
      }
    },
    {
      "source": "/about.html",
      "destination": "/about/",
      "statusCode": 301
    },
    {
      "source": "/privacy-policy.html",
      "destination": "/privacy-policy/",
      "statusCode": 301
    },
    {
      "source": "/terms.html",
      "destination": "/terms/",
      "statusCode": 301
    },
    {
      "source": "/contact.html",
      "destination": "/contact/",
      "statusCode": 301
    },
    {
      "source": "/tools/(.*)\\.html",
      "destination": "/tools/$1/",
      "statusCode": 301
    }
  ]
}